{
  "problemMatcher": [
    {
      "owner": "dotnet-format",
      "pattern": [
        {
          "regexp": "^(.+)\\((\\d+),(\\d+)\\):\\s+(error|warning|info)\\s+(.+):\\s+(.+)\\s+\\[.+\\]$",
          "file": 1,
          "line": 2,
          "column": 3,
          "severity": 4,
          "code": 5,
          "message": 6
        }
      ]
    }
  ]
}